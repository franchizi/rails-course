<%= link_to 'New', new_user_path %>

<% @users.each do |user| %>
    <div>
        <div>
            <h4><%= user.first_name %> <%= user.last_name %></h4>
        </div>
        <div>
            <p><%= user.email %></p>
        </div>
        <div>
            <img src='<%= user.picture %>' height='100'>
        </div>
        <%= link_to 'Edit', edit_user_path(user) %>
        <%= link_to 'Show', user_path(user) %>
        <%= link_to 'Delete', user_path(user), method: :delete %>
    </div>
<% end %>