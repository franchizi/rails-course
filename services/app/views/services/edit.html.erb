<%= form_for @service do |f| %>
    <div>
        <%= f.text_field :name, placeholder: 'Name' %>
    </div>
    <div>
        <%= f.text_field :logo, placeholder: 'Logo' %>
    </div>
    <div>
        <%= f.text_area :description, placeholder: 'Description' %>
    </div>
    <div>
        <%= f.submit 'Save' %>
        <p></p>
    </div>
<% end %>

    <div class='btn btn-success'>
        <%= link_to 'Cancel', services_path %>
    </div>